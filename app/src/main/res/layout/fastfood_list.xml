<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FastFood.FastfoodList">

    <ImageView
        android:id="@+id/mcdonald"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="143dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="11dp"
        android:scaleType="fitCenter"
        android:src="@drawable/macdonald"
        app:layout_constraintBottom_toTopOf="@+id/dunkin"
        app:layout_constraintEnd_toStartOf="@+id/burgerking"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/burgerking"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="143dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="11dp"
        android:scaleType="fitXY"
        android:src="@drawable/burgerking"
        app:layout_constraintBottom_toTopOf="@+id/pizzahut"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/mcdonald"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/dunkin"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="185dp"
        android:src="@drawable/dunkin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/pizzahut"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mcdonald" />

    <ImageView
        android:id="@+id/pizzahut"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="185dp"
        android:scaleType="fitXY"
        android:src="@drawable/pizzahut"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/dunkin"
        app:layout_constraintTop_toBottomOf="@+id/burgerking" />

</androidx.constraintlayout.widget.ConstraintLayout>